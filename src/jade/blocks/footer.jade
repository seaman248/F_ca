mixin social(fa)
	li
		a.social_link(href="#", class=fa)
			i.fa(class='fa-'+fa)

footer
	ul.footer-social
		+social('twitter')
		+social('facebook')
		+social('linkedin-square')
		+social('google-plus')
		+social('youtube')
